{
  "name": "file-manager",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --config vite/config.dev.ts --open",
    "preview": "vite preview --port=3000",
    "build": "run-p check-type build-prod",
    "build-prod": "rimraf dist && vite build --config vite/config.prod.ts",
    "check-type": "vue-tsc --noEmit",
    "test": "vitest",
    "lint": "eslint \"src/**/*.{vue,ts,tsx,cts,mts}\"",
    "fmt": "dprint fmt"
  },
  "dependencies": {
    "@alova/adapter-axios": "^2.0.4",
    "@types/node": "^22.0.0",
    "alova": "^3.0.4",
    "axios": "^1.7.2",
    "pinia": "^2.2.0",
    "pinia-plugin-persistedstate": "^3.2.1",
    "spark-md5": "^3.0.2",
    "tus-js-client": "^4.1.0",
    "vue": "^3.4.34",
    "vue-i18n": "^9.13.1",
    "vue-m-message": "^4.0.2",
    "vue-router": "^4.4.0",
    "vuetify": "3.6.14"
  },
  "devDependencies": {
    "@types/lodash": "^4.17.7",
    "@types/spark-md5": "^3.0.4",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@vitejs/plugin-legacy": "^5.4.1",
    "@vitejs/plugin-vue": "^5.1.1",
    "@vitejs/plugin-vue-jsx": "^4.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/tsconfig": "0.1.3",
    "@vueuse/core": "^10.11.0",
    "autoprefixer": "^10.4.19",
    "dprint": "^0.47.2",
    "eslint": "^8.57.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-vue": "^9.27.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^6.0.1",
    "rollup-plugin-visualizer": "^5.12.0",
    "sass": "^1.77.8",
    "typescript": "^5.5.4",
    "unplugin-auto-import": "^0.18.2",
    "unplugin-vue-components": "^0.27.3",
    "vite": "^5.3.5",
    "vite-plugin-checker": "latest",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-imagemin": "^0.6.1",
    "vite-plugin-mkcert": "^1.17.5",
    "vite-plugin-vuetify": "^2.0.3",
    "vitest": "^2.0.4",
    "vue-tsc": "^2.0.29"
  }
}
